main: main.c measure.c division.c create_csr.c algorithm_par.c core.c
  xlc_r -qsmp=omp -o main main.c measure.c division.c create_csr.c algorithm_par.c core.c -Wall -std=gnu11 -lm -fopenmp -O5 -qarch=pwr8 -qhot

test: test.c division.c create_csr.c algorithm_par.c core.c
  xlc_r -qsmp=omp -o test test.c division.c create_csr.c algorithm_par.c core.c -lm -std=gnu11 -fopenmp

.PHONY: clean
clean:
  rm -f *.o
  rm -f main
  rm -f test